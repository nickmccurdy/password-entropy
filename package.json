{
  "private": true,
  "dependencies": {
    "material-ui": "^1.0.0-beta.12",
    "material-ui-icons": "^1.0.0-beta.10",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.0.14",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "typeface-roboto": "^0.0.35"
  },
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-react": "^7.4.0",
    "npm-run-all": "^4.1.1",
    "react-test-renderer": "^16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint .",
    "tests": "react-scripts test --coverage",
    "test": "npm-run-all lint \"tests -- {*}\" --",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-shadow": "off",
      "semi": [
        "error",
        "never"
      ],
      "import/extensions": [
        "error",
        "always",
        {
          "css": "never",
          "js": "never",
          "jsx": "never"
        }
      ],
      "import/no-named-as-default": "off",
      "react/jsx-curly-brace-presence": "error",
      "react/jsx-filename-extension": "off"
    }
  },
  "eslintIgnore": [
    "build",
    "coverage"
  ],
  "homepage": "http://nickmccurdy.com/password-entropy",
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!src/index.js",
      "!**/coverage/**",
      "!**/build/**"
    ]
  }
}
