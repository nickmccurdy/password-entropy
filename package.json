{
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4",
    "object.entries": "^1.0.4",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.0.14",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "devDependencies": {
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-react": "^7.4.0",
    "jest-enzyme": "^4.0.0",
    "npm-run-all": "^4.1.1",
    "react-test-renderer": "^16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint .",
    "tests": "react-scripts test --coverage",
    "test": "npm-run-all lint \"tests -- {*}\" --",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-shadow": "off",
      "semi": [
        "error",
        "never"
      ],
      "import/no-named-as-default": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "coverage"
  ],
  "homepage": "http://nickmccurdy.com/password-entropy",
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!src/index.js",
      "!**/coverage/**",
      "!**/build/**"
    ]
  }
}
